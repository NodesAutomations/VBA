				</group>
				<group
					id="Group2"
					label="Normal">
					<box
						boxStyle="vertical"
						id="Box2">
						<checkBox
							id="Checkbox1"
							label="Checkbox1"
							getPressed="Checkbox_getPressed"
							onAction="Checkbox_onAction"/>
						<checkBox
							id="Checkbox2"
							label="Checkbox2"
							getPressed="Checkbox_getPressed"
							onAction="Checkbox_onAction"/>
