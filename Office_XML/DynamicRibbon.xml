							id="Dropdown1"
							label="Dropdown1"
							getItemCount="Dropdown_getItemCount"
							getItemLabel="Dropdown_getItemLabel"
							getSelectedItemIndex="Dropdown_getSelectedItemIndex"
							onAction="GetAction"/>
